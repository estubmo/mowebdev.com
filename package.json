{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "build": "tailwindcss -i ./src/styles/globals.css -o ./public/globals.css",
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun run liveReload\" \"bun run build --watch\"",
    "liveReload": "bunx beth-stack",
    "start": "bun run uno && bun run src/main.ts",
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/model/store/seed.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write ."
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^1.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.44.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@unocss/transformer-variant-group": "^0.55.7",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^8.6.0",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "pino": "^8.15.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-toolbelt": "latest",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@biomejs/biome": "^1.2.2",
    "@bogeychan/elysia-logger": "0.0.9",
    "@elysiajs/cron": "^0.6.0",
    "@elysiajs/static": "^0.6.0",
    "@iconify-json/logos": "^1.1.37",
    "@iconify-json/lucide": "^1.1.128",
    "@libsql/client": "0.3.5-pre.13",
    "@t3-oss/env-core": "^0.6.1",
    "@tlscipher/holt": "1.1.0",
    "beth-stack": "0.0.33",
    "drizzle-orm": "^0.28.6",
    "drizzle-typebox": "^0.1.1",
    "elysia": "0.7.4",
    "marked": "^9.1.0",
    "pino-pretty": "^10.2.0",
    "sanitize-html": "^2.11.0",
    "zod": "^3.22.2"
  }
